{
  "buildCommand": "bun run build",
  "framework": "nuxtjs",
  "functions": {
    "app/pages/**/*.vue": {
      "maxDuration": 30
    }
  },
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "memory": 3008,
        "maxDuration": 30
      }
    }
  ]
}